{
  "name": "aikom/api-client",
  "minimum-stability": "stable",
  "description": "This is a php library for working with API Aikom.",
  "keywords": [
    "api",
    "php",
    "aikom",
    "client",
    "curl"
  ],
  "license": "MIT",
  "version": "1.0.1",
  "authors": [
    {
      "name": "Alex Dubinin",
      "email": "keccgroup@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.3.0",
    "curl/curl": "^2.0"
  },
  "autoload": {
    "psr-4": {"Aikom\\": "src/"}
  },
  "prefer-stable": true,
  "homepage": "https://github.com/kfeniks",
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}